{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "error",
    "no-unused-vars": "warn",
    "no-nonoctal-decimal-escape": "off",
    "no-unsafe-optional-chaining": "off"
  }
}
